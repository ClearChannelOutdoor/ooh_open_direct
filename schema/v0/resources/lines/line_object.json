{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/Outsmart-OOH/ooh_open_direct/master/schema/v0/resources/lines/line_object.json",
    "type": "object",
    "title": "Line object",
    "description": "Template container for Line, still to be populated",
    "default": {},
    "additionalProperties": true,
    "properties": {
        "$schema": {
            "type": "string",
            "format": "uri",
            "description": "$schema only included to aid validation of JSON, should not be used in real calls."
        },
        "Id": {
            "$ref": "https://raw.githubusercontent.com/Outsmart-OOH/ooh_open_direct/master/schema/v0/resources/lines/line_id_string.json"
        },
        "Name": {
            "$id": "#/properties/Name",
            "type": "string",
            "title": "Line Name",
            "description": "The line's display name. The name must be unique."
        }
    }
}