{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/Outsmart-OOH/ooh_open_direct/master/schema/resources/orders/order_object.json",
    "type": "object",
    "title": "Order object",
    "description": "Template container for Order, still to be populated",
    "default": {},
    "additionalProperties": true,
    "properties": {
        "$schema": {
            "type": "string",
            "format": "uri",
            "description": "$schema only included to aid validation of JSON, should not be used in real calls."
        },
        "Id": {
            "$ref": "https://raw.githubusercontent.com/Outsmart-OOH/ooh_open_direct/master/schema/resources/orders/order_id_string.json"
        },
        "Name": {
            "$id": "#/properties/Name",
            "type": "string",
            "title": "Order Name",
            "description": "The order's display name. The name must be unique."
        }
    }
}